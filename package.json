{
  "name": "ai-chat-bot",
  "version": "1.0.0",
  "description": "Telegram бот с интеграцией OpenRouter API для доступа к GPT-4",
  "main": "dist/src/app.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/src/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "docker:build": "docker build -t ai-chat-bot .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "generate-key": "ts-node scripts/generate-key.ts"
  },
  "keywords": [
    "telegram",
    "bot",
    "ai",
    "openrouter",
    "gpt-4",
    "typescript"
  ],
  "author": "Pochipov Stas",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "axios": "^1.6.2",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "file-type": "^21.0.0",
    "ioredis": "^5.3.2",
    "mammoth": "^1.6.0",
    "multer": "2.0.2",
    "pdf-parse": "^1.1.1",
    "sharp": "^0.34.3",
    "telegraf": "^4.15.6",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.1",
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.1",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "prettier": "^3.1.0",
    "prisma": "^6.13.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=22.0.0",
    "yarn": ">=1.22.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
